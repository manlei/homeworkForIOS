<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dsca.cs.nju.mytabmenudemo">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".login.LoginActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name=".login.SignupActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity android:name=".publish.PublishDish" />
        <activity android:name=".classification.ClassificationDishes" />
        <activity android:name=".dish.DishShow" />
        <activity android:name=".message.MessageView"></activity>
    </application>

</manifest>